%div.login
  - if student_signed_in?
    %p= link_to 'My Settings', edit_student_path(current_student)
    %p= link_to 'Sign Out', destroy_student_session_path, :method => :delete
  - elsif employer_signed_in?
    %p= link_to 'My Settings', edit_employer_path(current_employer)
    %p= link_to 'Sign Out', destroy_employer_session_path, :method => :delete
  - else
    %p= link_to 'Student Sign In', new_student_session_path
    %p= link_to 'Employer Sign In', new_employer_session_path
